"source":"scripts/viz3/scales.js","original":{"line":60,"column":33}},{"generated":{"line":69,"column":51},"source":"scripts/viz3/scales.js","original":{"line":60,"column":51}},{"generated":{"line":69,"column":59},"source":"scripts/viz3/scales.js","original":{"line":60,"column":33}},{"generated":{"line":69,"column":60},"source":"scripts/viz3/scales.js","original":{"line":60,"column":9}},{"name":"domain","generated":{"line":69,"column":62},"source":"scripts/viz3/scales.js","original":{"line":60,"column":62}},{"generated":{"line":69,"column":68},"source":"scripts/viz3/scales.js","original":{"line":60,"column":9}},{"generated":{"line":69,"column":69},"source":"scripts/viz3/scales.js","original":{"line":60,"column":69}},{"generated":{"line":69,"column":70},"source":"scripts/viz3/scales.js","original":{"line":60,"column":70}},{"generated":{"line":69,"column":86},"source":"scripts/viz3/scales.js","original":{"line":60,"column":69}},{"generated":{"line":69,"column":88},"source":"scripts/viz3/scales.js","original":{"line":60,"column":88}},{"generated":{"line":69,"column":98},"source":"scripts/viz3/scales.js","original":{"line":60,"column":69}},{"generated":{"line":69,"column":100},"source":"scripts/viz3/scales.js","original":{"line":60,"column":100}},{"generated":{"line":69,"column":116},"source":"scripts/viz3/scales.js","original":{"line":60,"column":69}},{"generated":{"line":69,"column":117},"source":"scripts/viz3/scales.js","original":{"line":60,"column":9}},{"generated":{"line":69,"column":118},"source":"scripts/viz3/scales.js","original":{"line":60,"column":2}},{"generated":{"line":70,"column":0},"source":"scripts/viz3/scales.js","original":{"line":61,"column":1}}],"sources":{"scripts/viz3/scales.js":"/**\r\n * Defines the linear scale to use for the circle's radius.\r\n *\r\n * The area of the circle is linearly proportinal to the population of the given country.\r\n *\r\n * The area is a value defined in the interval [25, 400].\r\n *\r\n * @param {object} data2019 The data from 2019 to be displayed\r\n * @param {object} data2021 The data from 2021 to be displayed\r\n * @returns {*} The linear scale used to determine the radius\r\n */\r\nexport function setRadiusScale (data2019, data2021) {\r\n  const min2019 = d3.min(data2019, function (obj) {\r\n    return Math.min(obj.emploi_estimé)\r\n  })\r\n\r\n  const max2019 = d3.max(data2019, function (obj) {\r\n    return Math.max(obj.emploi_estimé)\r\n  })\r\n\r\n  const min2021 = d3.min(data2021, function (obj) {\r\n    return Math.min(obj.emploi_estimé)\r\n  })\r\n\r\n  const max2021 = d3.max(data2021, function (obj) {\r\n    return Math.max(obj.emploi_estimé)\r\n  })\r\n\r\n  const domain = [Math.min(min2019, min2021), Math.max(max2019, max2021)]\r\n  return d3.scaleLinear().range([25, 800]).domain(domain)\r\n}\r\n\r\n/**\r\n * Defines the band scale used to position the center of the circles in X.\r\n *\r\n * @param {number} width The width of the graph\r\n * @param {object} skills The data to be used\r\n * @returns {*} The linear scale in X\r\n */\r\nexport function setXScale (width, skills) {\r\n  return d3.scaleBand().range([0, width]).domain(skills)\r\n}\r\n\r\n/**\r\n * Defines the linear scale used to position the center of the circles in Y.\r\n *\r\n * @param {number} height The height of the graph\r\n * @returns {*} The linear scale in Y\r\n */\r\nexport function setYScale (height) {\r\n  return d3.scaleLinear().range([height, 0]).domain([0, 1])\r\n}\r\n\r\n/**\r\n * Defines the color scale used to fill the circles\r\n *\r\n * @returns {*} The ordinal scale in color\r\n */\r\nexport function setColorScale () {\r\n  return d3.scaleOrdinal().range(['red', 'orange', 'yellow']).domain(['Haut. Qualifié', 'Qualifié', 'Moins Qualifié'])\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"f5e3f68cf1e9f29cf6e40d2937047cf3","cacheData":{"env":{}}}